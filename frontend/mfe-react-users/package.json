{
  "name": "@marketplace/mfe-react-users",
  "version": "1.0.0",
  "description": "React micro-frontend for user management",
  "scripts": {
    "start": "webpack serve --port 4202",
    "start:standalone": "webpack serve --env standalone --port 4202",
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "analyze": "webpack-bundle-analyzer build/bundle-stats.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,scss}\"",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "single-spa",
    "react",
    "micro-frontend",
    "user-management"
  ],
  "author": "Marketplace Team",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "@babel/preset-typescript": "^7.23.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^25.0.9",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/styled-components": "^5.1.0",
    "@types/systemjs": "^6.13.0",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.8.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.0.0",
    "style-loader": "^3.3.0",
    "typescript": "^5.2.0",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.0",
    "webpack-config-single-spa-react-ts": "^4.0.0",
    "webpack-dev-server": "^4.15.0",
    "webpack-merge": "^5.10.0"
  },
  "dependencies": {
    "@marketplace/shared-utils": "file:../shared-utils",
    "axios": "^1.6.0",
    "framer-motion": "^10.16.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.18.0",
    "single-spa-react": "^6.0.0",
    "styled-components": "^6.1.0"
  },
  "main": "src/marketplace-mfe-react-users.tsx"
}
