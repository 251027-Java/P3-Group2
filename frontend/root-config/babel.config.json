{
  "presets": [
    // Code below fixes regenorater not defined error
    // Don't change it
    [
      "@babel/preset-env",
      {
        "targets": "defaults, not ie 11"
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "useESModules": true,
        "regenerator": false
      }
    ]
  ],
  "env": {
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": "current node"
          }
        ]
      ]
    }
  }
}
